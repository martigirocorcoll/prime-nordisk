<p id="notice"><%= notice %></p>

<h1>Pagos</h1>

<table>
  <thead>
    <tr>
      <th>Quantitat</th>
      <th>Fecha</th>
      <th>Pagador</th>
      <th>Expense</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pagos.each do |pago| %>
      <tr>
        <td><%= pago.quantitat %></td>
        <td><%= pago.fecha %></td>
        <td><%= pago.pagador_id %></td>
        <td><%= pago.expense_id %></td>
        <td><%= link_to 'Show', pago %></td>
        <td><%= link_to 'Edit', edit_pago_path(pago) %></td>
        <td><%= link_to 'Destroy', pago, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pago', new_pago_path %>
